{
  "editor.fontFamily": "'FiraCode Nerd Font', 'CaskaydiaCove Nerd Font Mono', 'JetBrainsMono Nerd Font','mononoki Nerd Font'",
  "editor.codeLensFontFamily": "'FiraCode Nerd Font', 'CaskaydiaCove Nerd Font Mono', 'JetBrainsMono Nerd Font','mononoki Nerd Font'",
  "editor.fontLigatures": false,
  "files.trimFinalNewlines": true,
  "files.insertFinalNewline": true,
  "explorer.confirmDelete": false,
  "files.trimTrailingWhitespace": true,
  "workbench.startupEditor": "none",
  "git.enableSmartCommit": true,
  "redhat.telemetry.enabled": false,
  "security.workspace.trust.untrustedFiles": "open",
  "editor.tabSize": 2,
  "editor.formatOnSave": true,
  "[python]": {
    "editor.formatOnType": true,
    "editor.defaultFormatter": "ms-python.black-formatter",
  },
  "extensions.ignoreRecommendations": true,
  "python.analysis.completeFunctionParens": true,
  "python.analysis.inlayHints.variableTypes": true,
  "python.analysis.inlayHints.functionReturnTypes": true,
  "python.analysis.inlayHints.callArgumentNames": "partial",
  "python.analysis.fixAll": [
    "source.convertImportFormat",
    "source.unusedImports"
  ],
  "gopls": {
    "ui.semanticTokens": true
  },
  "python.analysis.autoFormatStrings": true,
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "go.toolsManagement.autoUpdate": true,
  "git.ignoreMissingGitWarning": true,
  "git.openRepositoryInParentFolders": "never",
  "[terraform]": {
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "hashicorp.terraform"
  },
  "terraform.codelens.referenceCount": true,
  "terraform.experimentalFeatures.prefillRequiredFields": true,
  "diffEditor.maxComputationTime": 0,
  "files.associations": {
    "config": "bat",
    "*.ndjson": "json"
  },
  "terminal.integrated.defaultProfile.linux": "zsh",
  "window.titleBarStyle": "custom",
  "window.menuBarVisibility": "toggle",
  "editor.scrollbar.vertical": "hidden",
  "editor.scrollbar.verticalScrollbarSize": 0,
  "security.workspace.trust.startupPrompt": "never",
  "security.workspace.trust.enabled": false,
  "editor.minimap.side": "left",
  "extensions.autoUpdate": true,
  "workbench.statusBar.visible": true,
  "terminal.external.linuxExec": "kitty",
  "terminal.explorerKind": "both",
  "terminal.sourceControlRepositoriesKind": "both",
  "telemetry.telemetryLevel": "off",
  "explorer.confirmDragAndDrop": false,
  "window.zoomLevel": 1,
  "cSpell.userWords": [
    "Triebwerk"
  ],
  "continue.telemetryEnabled": false,
  "diffEditor.codeLens": true,
  "continue.enableTabAutocomplete": true,
  "continue.showInlineTip": true,
  "update.mode": "none",
  "workbench.sideBar.location": "right",
  "window.experimentalControlOverlay": false,
  "python.analysis.cacheLSPData": true
}
